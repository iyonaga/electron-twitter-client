{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": 0,
    "no-new": 0,
    "jsx-a11y/media-has-caption": 0,
    "prettier/prettier": ["error", {
      "singleQuote": true
    }],
    "import/no-extraneous-dependencies": ["error", {
      "devDependencies": true
    }],
    "react/forbid-prop-types": 0,
    "react/jsx-filename-extension": ["error", {
      "extensions": [".js", ".jsx"]
    }]
  }
}
